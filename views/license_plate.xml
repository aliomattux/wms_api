<odoo>
    <record id="view_license_plate_tree" model="ir.ui.view">
        <field name="name">License Plate Tree</field>
        <field name="model">license.plate</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="action_license_plates" model="ir.actions.act_window">
        <field name="name">License Plates</field>
        <field name="res_model">license.plate</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_id" ref="view_license_plate_tree"/>
    </record>
    <menuitem action="action_license_plates" id="menu_license_plate" parent="menu_wms_receiving" sequence="50"/>
</odoo>
