<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="product_empty_preferred_bin_form" model="ir.ui.view">
        <field name="name">Product Empty  Preferred Bin Form</field>
        <field name="model">product.empty.preferred.bin</field>
        <field name="arch" type="xml">
            <form string="">
                <notebook>
                    <page string="Products">
                        <group>
                            <field name="name" invisible="1" />
                            <field name="products_count" readonly="1" />
                        </group>
                        <group name="product_empty_preferred_bin" string="Empty Preferred Bins" readonly="1">
                            <field name="products" nolabel="1" widget="one2many_list" style="pointer-events:none;">
                                <tree readonly="1">
                                    <field name="product" invisible="1"/>
                                    <field name="ddn" />
                                    <field name="preferred_bin" />
                                    <field name="qty_available" />
                                    <field name="qty_demand" />
                                    <field name="stock_type" />
                                    <field name="qty_overstock" />
                                    <field name="qty_receiving" />
                                </tree>
                            </field>
                        </group>
                    </page>
                </notebook>
            </form>
        </field>
    </record>
    <record id="action_product_empty_preferred_bin" model="ir.actions.act_window">
        <field name="name">Empty Preferred Bins</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">product.empty.preferred.bin</field>
        <field name="view_mode">form</field>
    </record>
    <menuitem id="menu_products_empty_preferred_bin" parent="wms.menu_wms_reporting" action="action_product_empty_preferred_bin" />
</odoo>
